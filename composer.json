{
    "name": "gonzalocosin/signaturit",
    "authors": [
        {
            "name": "Gonzalo Cosin",
            "email": "gonzalocosin@gmail.com"
        }
    ],
    "require": {
        "ext-json": "*",
        "ext-pdo": "*",
        "beberlei/assert": "^v2.9.9",
        "php-di/php-di": "^6.3",
        "selective/basepath": "^2.0",
        "slim/psr7": "^1.3",
        "slim/slim": "4.*",
        "symfony/console": "^5.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpmd/phpmd": "^2.9.1",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    }
}
